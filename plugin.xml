    https://github.com/asennikov/BarcodeScannerPlugin.git
  </repo>
  <keywords>
    barcode, qrcode, qr, preview, camera, continuous, scanning
  </keywords>
  <license>
    MIT
  </license>
  <author>
    Alexander Sennikov
  </author>
  <engines>
    <engine name='cordova' version='>=3.0.0'/>
  </engines>
  <js-module src='www/BarcodeScanner.js' name='BarcodeScanner'>
